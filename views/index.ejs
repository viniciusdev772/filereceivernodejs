<!-- views/index.ejs -->

<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Lista de Usuários</title>
  </head>
  <body>
    <h1>Lista de Usuários</h1>
    <ul>
      <% usuarios.forEach(function(usuario) { %>
      <li>Email: <%= usuario.email %>, Nome: <%= usuario.nome %></li>
      <% }); %>
    </ul>
  </body>
</html>
